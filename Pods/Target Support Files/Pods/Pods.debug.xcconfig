CLANG_CXX_LANGUAGE_STANDARD = c++11
CLANG_CXX_LIBRARY = libc++
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/Colours" "${PODS_ROOT}/Headers/Public/MCFireworksButton" "${PODS_ROOT}/Headers/Public/PocketAPI" "${PODS_ROOT}/Headers/Public/Shimmer" "${PODS_ROOT}/Headers/Public/TUSafariActivity" "${PODS_ROOT}/Headers/Public/libHN" "${PODS_ROOT}/Headers/Public/pop"
OTHER_CFLAGS = $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/Colours" -isystem "${PODS_ROOT}/Headers/Public/MCFireworksButton" -isystem "${PODS_ROOT}/Headers/Public/PocketAPI" -isystem "${PODS_ROOT}/Headers/Public/Shimmer" -isystem "${PODS_ROOT}/Headers/Public/TUSafariActivity" -isystem "${PODS_ROOT}/Headers/Public/libHN" -isystem "${PODS_ROOT}/Headers/Public/pop"
OTHER_LDFLAGS = -ObjC -l"Pods-Colours" -l"Pods-MCFireworksButton" -l"Pods-PocketAPI" -l"Pods-Shimmer" -l"Pods-TUSafariActivity" -l"Pods-libHN" -l"Pods-pop" -l"c++" -framework "Foundation" -framework "Security"
OTHER_LIBTOOLFLAGS = $(OTHER_LDFLAGS)
PODS_ROOT = ${SRCROOT}/Pods